@{
}
@model List<EventCategory>

<h1>Event Category</h1>



            
<table class="table">
    <thead>
        <tr>
            <th scope="col">#</th>
            <th scope="col">Name</th>


        </tr>
    </thead>
    <tbody>
        @foreach (var meeting in Model)
        {
            <tr>



                <td>@meeting.Id</td>
                <td>@meeting.Name</td>






            </tr>

        }

    </tbody>
</table>



